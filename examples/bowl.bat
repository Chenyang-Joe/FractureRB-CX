@echo off
setlocal
set STARTTIME=%TIME%
@echo on



..\bin\FractureRB.exe bowl.bullet bowl.csv -o _out/bowl_ -n 500 -i 1e3 -f 5e6 --default-solver


@echo off
set ENDTIME=%TIME%
set /A T1=(1%STARTTIME:~0,2%-100)*360000 + (1%STARTTIME:~3,2%-100)*6000 + (1%STARTTIME:~6,2%-100)*100 + (1%STARTTIME:~9,2%-100)
set /A T2=(1%ENDTIME:~0,2%-100)*360000 + (1%ENDTIME:~3,2%-100)*6000 + (1%ENDTIME:~6,2%-100)*100 + (1%ENDTIME:~9,2%-100)
set /A DURATION=%T2%-%T1%
set /A DURATIONH=%DURATION% / 360000
set /A DURATIONM=(%DURATION% - %DURATIONH%*360000) / 6000
set /A DURATIONS=(%DURATION% - %DURATIONH%*360000 - %DURATIONM%*6000) / 100
set /A DURATIONC=(%DURATION% - %DURATIONH%*360000 - %DURATIONM%*6000 - %DURATIONS%*100)
if %DURATIONH% LSS 10 set DURATIONH=0%DURATIONH%
if %DURATIONM% LSS 10 set DURATIONM=0%DURATIONM%
if %DURATIONS% LSS 10 set DURATIONS=0%DURATIONS%
if %DURATIONC% LSS 10 set DURATIONC=0%DURATIONC%
echo START: %STARTTIME%
echo   END: %ENDTIME%
echo  DIFF: %DURATIONH%:%DURATIONM%:%DURATIONS%,%DURATIONC%

pause
